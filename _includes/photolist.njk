<div id="macy-container">
{%- for photo in photoslist | reverse -%}
  <div class='photo-list-item'>
	<a href="{{ photo.url }}" class="photolist-link">
		<img webc:is="eleventy-image" src="../static/img/{{photo.data.file_name}}" alt="Japanese Friendship Garden">
	</a>
</div>
{%- endfor -%}
</div>
<script src="https://cdn.jsdelivr.net/npm/macy@2"></script>
<script>
	Macy({
		container: '#macy-container',
		trueOrder: false,
		waitForImages: false,
		margin: 24,
		columns: 3,
		breakAt: {
			1200: 5,
			940: 3,
			520: 2,
			400: 1
		}
	});
</script>